{
  "name": "django-suit-dev",
  "version": "2.0.0",
  "description": "Django Suit development tools",
  "author": "Kaspars Sprogis",
  "scripts": {
    "clean": "rm -rf suit/static/suit",
    "build": "yarn clean && broccoli build suit/static/suit",
    "serve": "broccoli serve",
    "debug-build": "yarn clean && node $NODE_DEBUG_OPTION $(which broccoli) build suit/static/suit",
    "debug-serve": "node $NODE_DEBUG_OPTION $(which broccoli) serve"
  },
  "devDependencies": {
    "autosize": "^4.0.2",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "bootstrap": "^4.1.1",
    "broccoli": "^1.1.4",
    "broccoli-bs": "^0.1.0",
    "broccoli-clean-css": "^2.0.1",
    "broccoli-funnel": "^2.0.1",
    "broccoli-lint-eslint": "^3.3.2",
    "broccoli-livereload": "^1.3.0",
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-rollup": "^2.1.0",
    "broccoli-sass-source-maps": "^2.2.0",
    "broccoli-source": "^1.1.0",
    "broccoli-stew": "^1.5.0",
    "broccoli-uglify-sourcemap": "^2.2.0",
    "browser-sync": "^2.17.1",
    "font-awesome": "4.7.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.3.2",
    "npm-sass": "^2.2.3",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {}
}
